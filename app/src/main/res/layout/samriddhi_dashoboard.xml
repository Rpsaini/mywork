<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"

                android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp"
                android:orientation="horizontal">


                <RelativeLayout
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal">

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtMonthss"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_centerVertical="true"
                        android:backgroundTint="@color/color_black"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Month"
                        android:textColor="@color/color_black"
                        android:textSize="14sp" />
                    <!--     <TextView
                             android:layout_weight="1.5"
                             android:id="@+id/spinnerMonth"
                             android:layout_width="0dp"
                             android:layout_height="40dp"
                             android:layout_marginLeft="4dp"
                             android:layout_marginTop="2dp"
                             android:layout_marginRight="4dp"
                             android:background="@drawable/shape_line_botton"
                             android:drawableRight="@drawable/ic_down"
                             android:gravity="left|center"
                             android:hint="Select Month"
                             android:padding="5dp"
                             android:paddingLeft="5dp"
                             android:tag="Selection of Make is required."
                             android:textSize="@dimen/text_size_normal" />-->
                    <!--   <TextView
                           android:id="@+id/month"
                           android:layout_width="140dp"
                           android:layout_height="30dp"
                           android:layout_centerInParent="true"
                           android:layout_marginLeft="4dp"
                           android:layout_marginRight="4dp"
                           android:layout_toRightOf="@+id/txtMonths"
                           android:background="@drawable/shape_line_botton"
                           android:drawableRight="@drawable/ic_down"
                           android:gravity="left|center"
                           android:hint="Month"
                           android:tag="Selection of Make is required."
                           android:textSize="@dimen/text_size_normal" />-->
                    <Spinner
                        android:id="@+id/spinnerMonth"
                        android:layout_width="130dp"

                        android:layout_height="40dp"
                        android:layout_centerVertical="true"
                        android:layout_gravity="top"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/txtMonthss"
                        android:background="@drawable/spinner_background"
                        android:gravity="center"

                        ></Spinner>

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtTillDatess"

                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_centerInParent="true"
                        android:layout_marginLeft="20dp"
                        android:layout_toRightOf="@+id/spinnerMonth"
                        android:gravity="center"
                        android:text="Till Date"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtTillDate"
                        android:layout_width="90dp"
                        android:layout_height="40dp"

                        android:layout_centerInParent="true"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/txtTillDatess"
                        android:background="@drawable/shape_line_botton"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"

                    android:layout_marginTop="4dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.3"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Till"
                        android:textColor="@color/color_black"
                        android:textSize="14sp" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/color_light_gray"
                android:orientation="vertical"
                android:padding="6dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@color/color_light_gray"
                    android:orientation="horizontal"
                    android:padding="6dp">

                    <fontspackageForTextView.BoldTextView
                        android:id="@+id/txtBusinesssValueAchievemnets"

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Business Value Achievement"
                        android:textColor="@color/color_black"></fontspackageForTextView.BoldTextView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@color/colorLightBlack"
                    android:orientation="horizontal"
                    android:padding="6dp"
                    android:weightSum="3">

                    <fontspackageForTextView.Regular
                        android:id="@+id/greenTextView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.2"
                        android:gravity="left|center"
                        android:maxLines="1"
                        android:text="Mapped Stockists"
                        android:textColor="@color/colorWhite"
                        android:textSize="12sp" />

                    <fontspackageForTextView.Regular
                        android:id="@+id/blueTextView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"

                        android:layout_gravity="center"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.8"
                        android:gravity="center|left"
                        android:maxLines="2"
                        android:paddingLeft="10dp"
                        android:text="Target"
                        android:textColor="@color/colorWhite"

                        android:textSize="12sp" />

                    <fontspackageForTextView.Regular
                        android:id="@+id/orangetextView"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1"
                        android:gravity="center|left"
                        android:maxLines="2"
                        android:paddingLeft="20dp"
                        android:text="Achievement"
                        android:textColor="@color/colorWhite"

                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="12dp"
                    android:weightSum="3">

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtValueofMAppedStockist"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1.1"
                        android:gravity="center|left"
                        android:maxLines="1"
                        android:paddingLeft="10dp"
                        android:text="No of Mapped stockist"
                        android:textColor="@color/color_black"
                        android:textSize="15sp" />

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtValueTaget"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"

                        android:layout_weight="0.9"
                        android:gravity="center|left"
                        android:maxLines="1"
                        android:paddingLeft="10dp"
                        android:text="Target"
                        android:textColor="@color/color_black"

                        android:textSize="15sp" />

                    <fontspackageForTextView.BoldMedium
                        android:id="@+id/txtValueAchievment"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Achievement"
                        android:textColor="@color/color_black"

                        android:textSize="15sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="@color/color_light_gray"
                android:orientation="vertical"
                android:padding="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/colorPrimary"
                    android:orientation="horizontal"
                    android:padding="2dp">

                    <TextView
                        android:id="@+id/txtNumericAchievment"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="Numeric Achievement"
                        android:textColor="@color/colorWhite"
                        android:textSize="15sp"></TextView>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtThresholdTarget"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"
                        android:gravity="left"
                        android:maxLines="1"
                        android:text="Biling"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"
                        android:id="@+id/seekBarBilling"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:clickable="false"
                        android:enabled="false"
                        android:focusable="false"
                        android:max="100"
                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb"
                        android:thumbTint="@color/color_line" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtValueBilling"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium


                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"
                        android:gravity="left"
                        android:maxLines="1"
                        android:text="Threshold Target"
                        android:textColor="@color/color_black"
                     android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"

                        android:id="@+id/seekbarThreshhold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:enabled="false"

                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtThresholdTargetPerc"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtBusinessTarget"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"

                        android:gravity="left"
                        android:maxLines="1"
                        android:text="Business Target"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"

                        android:id="@+id/seekBarBusinessTarget"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtBusinesssTargetPercent"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium


                        android:id="@+id/txtRange"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"

                        android:gravity="left"
                        android:maxLines="1"
                        android:text="Range"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"

                        android:id="@+id/seekBarRange"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtRangePercent"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="center"

                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtBtxtFP1illing"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"
                        android:gravity="left"
                        android:maxLines="1"
                        android:text="FP1"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"
                        android:id="@+id/seekkBarFPOne"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtFPPercent"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:orientation="horizontal"
                    android:padding="10dp">

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtFP2"
                        android:layout_width="0dp"

                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="1.4"
                        android:gravity="left"
                        android:maxLines="1"
                        android:text="FP2"
                        android:textColor="@color/color_black"
                        android:textStyle="bold" />

                    <SeekBar
                        android:layout_gravity="center"

                        android:id="@+id/seekBarFBTwo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:progressDrawable="@drawable/seekbar_style"
                        android:thumb="@drawable/custom_thumb" />

                    <fontspackageForTextView.BoldMedium

                        android:id="@+id/txtFP2AchievmentsPercentage"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_weight="0.5"
                        android:ellipsize="end"
                        android:gravity="right"
                        android:maxLines="1"
                        android:text="0%"

                        android:textColor="@color/color_black" />
                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/tempTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/colorWhite"
                android:visibility="gone" />

            <fontspackageForTextView.Regular
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:id="@+id/txtActionable"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/ll_sign_in"
                android:layout_centerHorizontal="true"
                android:layout_margin="30dp"
                android:layout_marginTop="20dp"
                android:background="@color/redcolor"
                android:contentDescription=""
                android:gravity="center"
                android:minHeight="10dp"
                android:padding="10dp"
                android:text="Actionable Stockist list >>"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/text_size_normal"
                android:visibility="visible" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>